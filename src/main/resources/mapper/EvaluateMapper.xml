<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cc.wuque.mapper.EvaluateMapper">
        <insert id="addEvaluate" >
            insert into
                p_evaluate(oid,b_uid,s_uid,content,stater,target,pid)
                value
                (#{oid},#{b_uid},#{s_uid},#{content},#{stater},#{target},#{pid})
        </insert>
</mapper>